@article{chasco2017,
  title = {Estimates of {{Chinook}} Salmon Consumption in {{Washington State}} Inland Waters by Four Marine Mammal Predators from 1970 to 2015},
  volume = {74},
  issn = {0706-652X},
  doi = {10.1139/cjfas-2016-0203},
