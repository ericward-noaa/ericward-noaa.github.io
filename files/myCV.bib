  timestamp = {2018-02-03T22:49:48Z},
  number = {8},
  journal = {Canadian Journal of Fisheries and Aquatic Sciences},
  author = {Chasco, Brandon and Kaplan, Isaac C. and Thomas, Austen and Acevedo-Guti{\'e}rrez, Alejandro and Noren, Dawn and Ford, Michael J. and Hanson, M. Bradley and Scordino, Jonathan and Jeffries, Steve and Pearson, Scott and Marshall, Kristin N. and Ward, Eric J.},
