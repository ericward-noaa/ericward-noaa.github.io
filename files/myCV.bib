
@article{ward2005,
  title = {Differences between Fishery-Dependent and Fishery-Independent Estimates of Single-and Mixed-Species Dolphin Schools: Implications for Single-Species Stock Assessments},
  volume = {21},
  issn = {1748-7692},
  shorttitle = {Differences {{Between Fishery}}-{{Dependent}} and {{Fishery}}-{{Independent Estimates}} of {{Single}}-and {{Mixed}}-{{Species Dolphin Schools}}},
  doi = {10.1111/j.1748-7692.2005.tb01223.x},
  abstract = {Since 1979, fishery-independent data and fishery-dependent data have been used to estimate trends in the abundance of the northeastern stock of spotted dolphins in the eastern tropical Pacific. Data collected aboard tuna vessels have been used to estimate trends in relative abundance, while data collected from fishery-independent research vessels have been used to estimate trends in actual abundance. One of the largest discrepancies between the two data sources is that tuna vessels tend to report dolphin schools that are 400\%\textemdash{}500\% larger than schools observed by research vessels. After comparing research vessel and tuna vessel observations overlapping in space and time, it appears that either measurement error or selective reporting of large schools is the most likely explanation for the disparity. Comparing single-species schools against portions of mixed- species schools introduced bias. Revised fishery-dependent abundance estimates were generated using only single-species dolphin schools, resulting in estimates that appear more similar to recent fishery-independent abundance estimates. correctable in fishery-dependent data, suggesting that data on mixed-species schools should not be considered if fishery-dependent data are to be combined with fishery-independent data for stock assessment purposes.},
  language = {en},
  timestamp = {2018-02-03T22:08:50Z},
  number = {2},
  journal = {Marine Mammal Science},
  author = {Ward, Eric J.},
  month = apr,
  year = {2005},
  keywords = {abundance estimation,line-transects,school size,Stenella attenuata,stock assessment,trend analysis,tuna-dolphin},
  pages = {189--203},
  file = {Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/G4U3XXIT/abstract.html:text/html}
}

@article{branch2006,
  title = {Fleet Dynamics and Fishermen Behavior: Lessons for Fisheries Managers},
